<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #FFFFFF;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bd.edu.seu.office_management.Controller.LoginController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="446.0" pickOnBounds="true" smooth="false">
         <image>
            <Image url="@../../../../Image/official-login.jpg" />
         </image>
      </ImageView>
      <Label alignment="TOP_CENTER" layoutX="456.0" layoutY="33.0" prefHeight="33.0" prefWidth="255.0" text="LogIn">
         <font>
            <Font name="Arial Black" size="26.0" />
         </font>
      </Label>
      <TextField fx:id="nameField" layoutX="456.0" layoutY="150.0" prefHeight="33.0" prefWidth="255.0" />
      <Label layoutX="456.0" layoutY="119.0" prefHeight="18.0" prefWidth="148.0" text="Username:">
         <font>
            <Font name="Arial Bold" size="15.0" />
         </font></Label>
      <PasswordField fx:id="passField" layoutX="456.0" layoutY="229.0" prefHeight="33.0" prefWidth="255.0" />
